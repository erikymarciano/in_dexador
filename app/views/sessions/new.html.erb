<% @title="Nova Sessão" %>
<h1>Nova sessão</h1>
<!--<p>Find me in app/views/sessions/new.html.erb</p>-->

<%= form_for(:session, url: login_path) do |f| %>

<%= f.label :email %>

<%= f.email_field :email %>

<%= f.label :password %>

<%= f.password_field :password %>

<%= f.submit "Log in" %>

<% end %>
